<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>crdt</Title>
			<Shortcut>crdt</Shortcut>
			<Description>Code snippet for DateTime date = Console.ReadLine</Description>
			<Author>Me</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="false">
					<ID>SystemConsole</ID>
					<Function>SimpleTypeName(global::System.Console)</Function>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[DateTime date = DateTime.ParseExact($SystemConsole$.ReadLine(), "d-M-yyyy", CultureInfo.InvariantCulture);$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>